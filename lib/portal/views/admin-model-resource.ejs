angular.module('<%=model.module.resource%>', ['ngResource'])
  .factory('<%=model.name%>', function($resource, $http) {
    var temp = $resource('<%=model.route%>', 
                          null, 
                          {
                            create: {
                              method: '<%=createVerb%>'
                            }
                          });
    temp.prototype.create = function(cb)
    {
      return temp.create({id: undefined}, angular.extend({}, this, {_id:undefined}), cb);
    };
    temp.prototype.push = function(path, obj)
    {
      return $http.<%=createVerb%>(path, obj, {headers: {'Content-Type': 'application/json'}});
    };
    temp.prototype.pop = function(path)
    {
      return $http.delete(path, null, {headers: {'Content-Type': 'application/json'}});
    };
    temp.prototype.set = function()
    {
    };
    temp.prototype.save = function(cb) {};

    return temp;
  });